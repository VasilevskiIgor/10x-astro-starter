{
  "info": {
    "_postman_id": "vibetravels-api-v1",
    "name": "VibeTravels API",
    "description": "Complete API collection for VibeTravels MVP - Travel planning with AI generation",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3002",
      "type": "string"
    },
    {
      "key": "supabase_url",
      "value": "http://127.0.0.1:54321",
      "type": "string"
    },
    {
      "key": "anon_key",
      "value": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24iLCJleHAiOjE5ODM4MTI5OTZ9.CRXP1A7WOeoJeXxjNni43kdQwgnWNReilDMblYTn_I0",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    },
    {
      "key": "trip_id",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Sign Up",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set(\"jwt_token\", jsonData.access_token);",
                  "    console.log(\"JWT Token saved:\", jsonData.access_token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "apikey",
                "value": "{{anon_key}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"postman-test@example.com\",\n  \"password\": \"SecurePass123!\"\n}"
            },
            "url": {
              "raw": "{{supabase_url}}/auth/v1/signup",
              "host": ["{{supabase_url}}"],
              "path": ["auth", "v1", "signup"]
            },
            "description": "Register a new user and automatically save JWT token"
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set(\"jwt_token\", jsonData.access_token);",
                  "    console.log(\"JWT Token saved:\", jsonData.access_token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "apikey",
                "value": "{{anon_key}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"postman-test@example.com\",\n  \"password\": \"SecurePass123!\"\n}"
            },
            "url": {
              "raw": "{{supabase_url}}/auth/v1/token?grant_type=password",
              "host": ["{{supabase_url}}"],
              "path": ["auth", "v1", "token"],
              "query": [
                {
                  "key": "grant_type",
                  "value": "password"
                }
              ]
            },
            "description": "Login existing user and save JWT token"
          }
        }
      ]
    },
    {
      "name": "Trips",
      "item": [
        {
          "name": "Create Trip",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201 || pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set(\"trip_id\", jsonData.id);",
                  "    console.log(\"Trip ID saved:\", jsonData.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"destination\": \"Tokyo, Japan\",\n  \"start_date\": \"2025-06-01\",\n  \"end_date\": \"2025-06-07\",\n  \"description\": \"First time in Japan, interested in culture, food, and technology. Would like to visit temples, try authentic ramen, and see modern Tokyo.\",\n  \"generate_ai\": false\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/trips",
              "host": ["{{base_url}}"],
              "path": ["api", "trips"]
            }
          }
        },
        {
          "name": "List Trips",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/trips?limit=20&offset=0",
              "host": ["{{base_url}}"],
              "path": ["api", "trips"],
              "query": [
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "offset",
                  "value": "0"
                },
                {
                  "key": "status",
                  "value": "completed",
                  "disabled": true
                },
                {
                  "key": "sort",
                  "value": "created_at:desc",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Get Trip by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}"]
            }
          }
        },
        {
          "name": "Update Trip",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"description\": \"Updated: First time in Japan, very excited about ramen and temples!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}"]
            }
          }
        },
        {
          "name": "Delete Trip",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "AI Generation",
      "item": [
        {
          "name": "Generate AI - Default",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}"
            },
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}/generate-ai",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}", "generate-ai"]
            },
            "description": "Generate AI itinerary with defaults. May take 20-60 seconds!"
          }
        },
        {
          "name": "Generate AI - Custom Params",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"model\": \"gpt-3.5-turbo\",\n  \"temperature\": 0.8\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}/generate-ai",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}", "generate-ai"]
            },
            "description": "Generate with custom temperature. May take 20-60 seconds!"
          }
        },
        {
          "name": "Regenerate AI - Force",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{jwt_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"force_regenerate\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/trips/{{trip_id}}/generate-ai",
              "host": ["{{base_url}}"],
              "path": ["api", "trips", "{{trip_id}}", "generate-ai"]
            },
            "description": "Force regeneration (overwrites existing). May take 20-60 seconds!"
          }
        }
      ]
    }
  ]
}
